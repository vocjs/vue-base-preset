{
  "vueVersion": "3",
  "useConfigFiles": true,
  "cssPreprocessor": "dart-sass",
  "plugins": {
    "@vue/cli-plugin-babel": {},
    "@vue/cli-plugin-typescript": {
      "classComponent": true,
      "useTsWithBabel": true
    },
    "@vue/cli-plugin-router": {
      "historyMode": true
    },
    "@vue/cli-plugin-vuex": {},
    "@vue/cli-plugin-eslint": {
      "config": "prettier",
      "lintOn": [
        "commit"
      ]
    },
    "@vue/cli-plugin-unit-jest": {},
    "vue-cli-plugin-extend-base": {
      "eslint": true,
      "forkTsChecker": {}
    },
    "vue-cli-plugin-mocker": {
      "prompts": true,
      "dir": "mock",
      "demo": true,
      "mock": {}
    }
  }
}
